// factorial of n in Ascending order
// n!
// 1x2x3x4x5=5!
#include<stdio.h>
void main()
 {
 	int n, f, i;
 	scanf("%d", &n);
 	i=1;
 	f=1;
 	while(i<=n)   //f=fxi
 	{
 	 f=f*i;
	 i++;	
	 }
	 printf("\nFactorial = %d", f);
 }
 
 
 // factorial of n in Descending order
// n!
// 5x4x3x2x1=5!
#include<stdio.h>
void main()
 {
 	int n, f;
 	scanf("%d", &n);
 	f=1;
 	while(n>0)   //f=fxn
 	{
 	 f=f*n;
	 n--;	
	 }
	 printf("\nFactorial = %d", f);
 }
 
