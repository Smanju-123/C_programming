/* 
total marks of a student
total and average

input : s1, s2, s3, s4, s5
process : total = s1+s2+s3+s4+s5
output : total
*/

#include<stdio.h>
void main()
{
	int s1, s2, s3, s4, s5, tot, avg;
	scanf("%d %d %d %d %d", &s1, &s2, &s3, &s4, &s5);
	tot = s1+s2+s3+s4+s5;
	avg = tot/5;
	printf("Total=%d", tot);
	printf("\nAverage=%d", avg);
	
}
