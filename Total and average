//total marks of a student
// total marks and average
/* */

#include<stdio.h>
void main(){
	int s1, s2, s3, s4, s5, tot;
	float avg;                // As avg can be decimal
	scanf("%d", &s1);
	scanf("%d", &s2);
	scanf("%d", &s3);            // total is 5
	scanf("%d", &s4);
	scanf("%d", &s5);
	tot = s1 + s2 + s3 + s4 + s5;
	avg = tot/5;
	printf("Total = %d\n", tot);
	printf("Average = %.2f\n", avg);
	
}
